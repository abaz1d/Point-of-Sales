<!doctype html>
<html lang="en">

<head>

    <meta charset="utf-8" />
    <title>Skote - Admin & Dashboard Template</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta content="Premium Multipurpose Admin & Dashboard Template" name="description" />
    <meta content="Themesbrand" name="author" />
    <!-- App favicon -->
    <link rel="shortcut icon" href="assets/images/favicon.ico">

    <link href="/fontawesome/css/all.css" rel="stylesheet" />
    <link rel="stylesheet" href="/bootstrap-5.2.0/css/bootstrap.min.css" />
    <link rel="stylesheet" href="stylesheet/style.css" />
    <script src="/bootstrap-5.2.0/js/bootstrap.min.js"></script>
    <script src="/javascripts/jquery-3.6.0.min.js"></script>
    <!-- App favicon -->
    <link rel="shortcut icon" href="/">

    <!-- Bootstrap Css -->
    <link href="/assets/css/bootstrap.min.css" id="bootstrap-style" rel="stylesheet" type="text/css" />
    <!-- Icons Css -->
    <link href="/assets/css/icons.min.css" rel="stylesheet" type="text/css" />
    <!-- App Css-->
    <link href="/assets/css/app.min.css" id="app-style" rel="stylesheet" type="text/css" />
</head>

<body data-sidebar="dark">

    <!-- <body data-layout="horizontal" data-topbar="dark"> -->

    <!-- Begin page -->
    <div id="layout-wrapper">

        <div>

            <header id="page-topbar">
                <div class="navbar-header">
                    <div class="d-flex">

                        <button type="button" href="/utama#" class="btn btn-sm px-3 font-size-16 header-item">
                            <i class="fa-solid fa-angle-left"></i>
                        </button>

                        <!-- App Search-->
                        <form class="app-search d-none d-lg-block">
                            <div class="position-relative">
                                <input type="text" class="form-control" placeholder="Search...">
                                <span class="fa-solid fa-magnifying-glass"></span>
                            </div>
                        </form>

                    </div>

                    <div class="d-flex">

                        <div class="dropdown d-inline-block d-lg-none ms-2">
                            <button type="button" class="btn header-item noti-icon waves-effect"
                                id="page-header-search-dropdown" data-bs-toggle="dropdown" aria-haspopup="true"
                                aria-expanded="false">
                                <i class="fa-solid fa-magnifying-glass"></i>
                            </button>
                            <div class="dropdown-menu dropdown-menu-lg dropdown-menu-end p-0"
                                aria-labelledby="page-header-search-dropdown">

                                <form class="p-3">
                                    <div class="form-group m-0">
                                        <div class="input-group">
                                            <input type="text" class="form-control" placeholder="Search ..."
                                                aria-label="Recipient's username">
                                            <div class="input-group-append">
                                                <button class="btn btn-primary" type="submit"><i
                                                        class="fa-solid fa-magnifying-glass"></i></button>
                                            </div>
                                        </div>
                                    </div>
                                </form>
                            </div>
                        </div>

                        <div class="dropdown d-inline-block">
                            <button type="button" class="btn header-item waves-effect" id="page-header-user-dropdown"
                                data-bs-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                <img class="rounded-circle header-profile-user" src="/assets/images/users/avatar-1.jpg"
                                    alt="Header Avatar">
                                <span class="d-none d-xl-inline-block ms-1" key="t-henry">Henry</span>
                                <i class="fa-solid fa-chevron-down"></i>
                            </button>
                            <div class="dropdown-menu dropdown-menu-end">
                                <!-- item-->
                                <a class="dropdown-item" href="/utama#"><i class="fa-solid fa-user-tie"></i> <span
                                        key="t-profile">Profile</span></a>
                                <a class="dropdown-item" href="/utama#"><i class="fa-solid fa-wallet"></i> <span
                                        key="t-my-wallet">My Wallet</span></a>
                                <div class="dropdown-divider"></div>
                                <a class="dropdown-item text-danger" href="/utama#"><i
                                        class="fa-solid fa-right-to-bracket"></i> <span key="t-logout">Logout</span></a>
                            </div>
                        </div>

                    </div>
                </div>
            </header>

            <!-- ========== Left Sidebar Start ========== -->

            <!-- Left Sidebar End -->
        </div>


        <!-- ============================================================== -->
        <!-- Start right Content here -->
        <!-- ============================================================== -->


        <div class="page-content">
            <div class="container-fluid">

                <!-- start page title -->
                <div class="row">
                    <div class="col-12">
                        <div class="page-title-box d-sm-flex align-items-center justify-content-between">
                            <h4 class="mb-sm-0 font-size-18">Edit Gudang</h4>

                            <div class="page-title-right">
                                <ol class="breadcrumb m-0">
                                    <li class="breadcrumb-item"><a href="javascript: void(0);">Gudang</a></li>
                                    <li class="breadcrumb-item active">Edit Gudang</li>
                                </ol>
                            </div>

                        </div>
                    </div>
                </div>
                <!-- end page title -->

                <div class="row">
                    <div class="col-12">
                        <div class="card">
                            <div class="card-body">

                                <form action="" method="post" id="form-user">

                                    <div class="row mb-3">
                                        <label for="id_gudang" class="col-sm-2 col-form-label">ID Gudang</label>
                                        <div class="col-sm-10">
                                            <input type="text" class="form-control" id="id_gudang" value="<%=item.id_gudang%>"
                                                name='id_gudang' disabled>
                                        </div>
                                    </div>

                                    <div class="row mb-3">
                                        <label for="nama_gudang" class="col-sm-2 col-form-label">Nama Gudang</label>
                                        <div class="col-sm-10">
                                            <input type="text" class="form-control" id="nama_gudang" value="<%=item.nama_gudang%>"
                                                name='nama_gudang' required>
                                        </div>
                                    </div>

                                    <div class="row mb-3">
                                        <label for="alamat_gudang" class="col-sm-2 col-form-label">Alamat Gudang</label>
                                        <p class="card-title-desc">Contoh : <br> Jl. H. Mahdi II No.89, Cisaranten
                                            Wetan, Kec. Cinambo, ota Bandung, Jawa Barat, 40293
                                        <div>
                                            <input type="text" class="form-control" id="alamat_gudang" value="<%=item.alamat_gudang%>"
                                            name='alamat_gudang' required>
                                        </div>
                                    </div>

                                    <div class="card-footer">
                                        <button type="submit" class="btn btn-primary">Save</button>

                                        <a href="/gudang" class="btn btn-warning">Cancel</a>
                                    </div>

                                </form>

                            </div>
                        </div>
                    </div>
                </div>
                <!-- end row -->

            </div> <!-- container-fluid -->
        </div>
        <!-- End Page-content -->


        <!-- end main content-->

    </div>
    <!-- END layout-wrapper -->


    <!-- JAVASCRIPT -->
    <script src="assets/libs/jquery/jquery.min.js"></script>
    <script src="assets/libs/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="assets/libs/metismenu/metisMenu.min.js"></script>
    <script src="assets/libs/simplebar/simplebar.min.js"></script>
    <script src="assets/libs/node-waves/waves.min.js"></script>

    <!-- select 2 plugin -->
    <script src="https://themesbrand.com/skote/layouts/assets/libs/select2/js/select2.min.js"></script>

    <!-- dropzone plugin -->
    <script src="https://themesbrand.com/skote/layouts/assets/libs/dropzone/min/dropzone.min.js"></script>

    <!-- init js -->
    <script src="https://themesbrand.com/skote/layouts/assets/js/pages/ecommerce-select2.init.js"></script>

    <!-- App js -->
    <script src="assets/js/app.js"></script>

</body>

</html>