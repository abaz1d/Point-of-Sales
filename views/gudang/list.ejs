<%- include('../partials/header') %>

        <!-- ============================================================== -->
        <!-- Start right Content here -->
        <!-- ============================================================== -->
        

        <div class="main-content">

            <div class="page-content">
                <div class="container-fluid">

                    <!-- start page title -->
                    <div class="row">
                        <div class="col-12">
                            <div class="page-title-box d-sm-flex align-items-center justify-content-between">
                                <h4 class="mb-sm-0 font-size-18">Gudang</h4>

                                <div class="page-title-right">
                                    <ol class="breadcrumb m-0">
                                        <li class="breadcrumb-item"><a href="javascript: void(0);">Gudang</a></li>
                                        <li class="breadcrumb-item active">List</li>
                                    </ol>
                                </div>

                            </div>
                        </div>
                    </div>
                    <!-- end page title -->

                    <div class="row">
                        <div class="col-xl-4">
                            <!-- ==================Searching = -->

                            <div class="card">
                                <div class="card-body">
                                    <h4 class="card-title mb-4">Pencarian Data</h4>
                                    <div class="row">
                                        <form id="form-search">

                                            <div class="row mb-3">
                                                <label for="searchString"
                                                    class="col-sm-2 col-form-label">Invoice</label>
                                                <div class="col-sm-10">
                                                    <input type="text" class="form-control" id="searchString"
                                                        name="searchString" placeholder="Tulis Invoice">
                                                </div>
                                            </div>

                                            <div class="row mb-3">
                                                <label for="searchDate" class="col-sm-2 col-form-label" id="searchDate"
                                                    name="searchDate" for="searchDate">Date</label>
                                                <div class="col-sm-4">
                                                    <input type="date" id="searchStartDate" name="searchStartDate"
                                                        class="form-control" placeholder="Tulis Date">
                                                    <label class="col-sm-4 col-form-label">to</label>
                                                    <input type="date" id="searchEndDate" name="searchEndDate"
                                                        class="form-control" placeholder="Tulis Date">
                                                </div>
                                            </div>

                                            <div class="card-footer">
                                                <button type="submit" class="btn btn-primary">Cari</button>

                                                <a href="/jquery.html" class="btn btn-warning">Reset</a>
                                            </div>

                                        </form>
                                    </div>

                                </div>
                            </div>
                        </div>

                        <div class="col-xl-8">
                            <div class="card">
                                <div class="card-body">
                                    
            
                                    <div class="table-responsive">
                                        <table class="table align-middle table-nowrap table-check">
                                            <thead class="table-light">
                                                <tr>
                                                    <th class="align-middle">No</th>
                                                    <th class="align-middle">ID Gudang</th>
                                                    <th class="align-middle">Nama Gudang</th>
                                                    <th class="align-middle">Alamat</th>
                                                    <th class="align-middle">Actions</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <% rows.forEach((item, index)=> { %>
                                                <tr>
                                                    <td>
                                                        <%=index + 1%>
                                                    </td>
                                                    <td>
                                                        <%=item.id_gudang%>
                                                    </td>
                                                    <td>
                                                        <%=item.nama_gudang%>
                                                    </td>
                                                    <td>
                                                        <%=item.alamat_gudang%>
                                                    </td>
                                                    <td>
                                                        <div class="d-flex gap-3">
                                                            <a href="javascript:void(0);" class="text-pri"><i class="fa-solid fa-print"></i></a>
                                                            <a href="/gudang/edit/<%= item.id_gudang %>" class="text-success"><i class="fa-solid fa-pencil"></i></a>
                                                            <a href="/gudang/edit/<%= item.id_gudang %>" class="text-danger"><i class="fa-solid fa-trash"></i></a>
                                                        </div>
                                                    </td>
                                                </tr>
                                                <% }) %>
                                            </tbody>
                                        </table>
                                    </div>

                                    <div class="row mb-2">
                                        <div class="col-sm-4">
                                            <div class="text-sm-left">
                                                <a href="/gudang/add" class="btn btn-success btn-rounded waves-effect waves-light mb-2 me-2"><i class="fa-solid fa-plus"></i> Tambah Gudang </a>
                                            </div>
                                        </div>
                                        <div class="col-sm-8">
                                            <ul class="pagination pagination-rounded justify-content-end mb-2">
                                                <li class="page-item disabled">
                                                    <a class="page-link" href="javascript: void(0);" aria-label="Previous">
                                                        <i class="fa-solid fa-angle-left"></i>
                                                    </a>
                                                </li>
                                                <li class="page-item active"><a class="page-link" href="javascript: void(0);">1</a></li>
                                                <li class="page-item"><a class="page-link" href="javascript: void(0);">2</a></li>
                                                <li class="page-item"><a class="page-link" href="javascript: void(0);">3</a></li>
                                                <li class="page-item"><a class="page-link" href="javascript: void(0);">4</a></li>
                                                <li class="page-item"><a class="page-link" href="javascript: void(0);">5</a></li>
                                                <li class="page-item">
                                                    <a class="page-link" href="javascript: void(0);" aria-label="Next">
                                                        <i class="fa-solid fa-angle-right"></i>
                                                    </a>
                                                </li>
                                            </ul>
                                        </div><!-- end col-->
                                    </div>

                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- end row -->
                </div>
                <!-- container-fluid -->
            </div>
            <!-- End Page-content -->

            <footer class="footer">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-sm-6">
                            <script>document.write(new Date().getFullYear())</script> Â© Skote.
                        </div>
                        <div class="col-sm-6">
                            <div class="text-sm-end d-none d-sm-block">
                                Design & Develop by Themesbrand
                            </div>
                        </div>
                    </div>
                </div>
            </footer>
        </div>
        <script>

            
        </script>
        <!-- end main content-->

        <%- include('../partials/footer') %>