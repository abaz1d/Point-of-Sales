<%- include('../partials/header') %>

        <!-- ============================================================== -->
        <!-- Start right Content here -->
        <!-- ============================================================== -->
        

        <div class="main-content">

            <div class="page-content">
                <div class="container-fluid">

                    <!-- start page title -->
                    <div class="row">
                        <div class="col-12">
                            <div class="page-title-box d-sm-flex align-items-center justify-content-between">
                                <h4 class="mb-sm-0 font-size-18">Gudang</h4>

                                <div class="page-title-right">
                                    <ol class="breadcrumb m-0">
                                        <li class="breadcrumb-item"><a href="javascript: void(0);">Gudang</a></li>
                                        <li class="breadcrumb-item active">List</li>
                                    </ol>
                                </div>

                            </div>
                        </div>
                    </div>
                    <!-- end page title -->

                    <div class="row">
                        <div class="col-xl-4">
                            <!-- ==================Searching = -->

                            <div class="card">
                                <div class="card-body">
                                    <h4 class="card-title mb-4">Pencarian Data</h4>
                                    <div class="row">
                                        <form id="form-search">
                                            <input type="hidden" name="page" value="1">
                                            <div class="row mb-3">
                                                <label for="cari_id"
                                                    class="col-sm-4 col-form-label">ID Gudang</label>
                                                <div class="col-sm-8">
                                                    <input type="text" class="form-control" id="cari_id"
                                                        name="cari_id" placeholder="<%=query.cari_id==undefined ? 'Tulis ID Gudang' : 'Tulis ID Gudang'%>" 
                                                        value="<%=query.cari_id%>">
                                                </div>
                                            </div>

                                            <div class="row mb-3">
                                                <label for="cari_nama"
                                                    class="col-sm-4 col-form-label">Nama Gudang</label>
                                                <div class="col-sm-8">
                                                    <input type="text" class="form-control" id="cari_nama"
                                                        name="cari_nama" placeholder="Tulis Nama Gudang" 
                                                        value="<%=query.cari_nama%>">
                                                </div>
                                            </div>
                                           

                                            <div class="card-footer">
                                                <button type="submit" class="btn btn-primary">Cari</button>

                                                <a href="/gudang" class="btn btn-warning">Reset</a>
                                            </div>

                                        </form>
                                    </div>

                                </div>
                            </div>
                        </div>

                        <div class="col-xl-8">
                            <div class="card">
                                <div class="card-body">
                                    
            
                                    <div class="table-responsive">
                                        <table class="table align-middle table-nowrap table-check">
                                            <thead class="table-light">
                                                <tr>
                                                    <th class="align-middle">
                                                    <a href="<%=`/gudang?page=${page}${filter}`%><%= sortBy == 'id_gudang' && sortMode == 'asc' ? "&sortBy=id_gudang&sortMode=desc" : "&sortBy=id_gudang&sortMode=asc" %>">
                                                        <i
                                                            class="fa-solid fa-<%= query.sortBy == 'id_gudang' ? query.sortMode == 'asc' ? 'arrow-up-1-9' : 'arrow-up-9-1' : 'sort' %>"></i>
                                                        ID Gudang
                                                    </a>
                                                    </th>
                                                    <th class="align-middle">Nama Gudang</th>
                                                    <th class="align-middle">Alamat</th>
                                                    <th class="align-middle">Actions</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <% rows.forEach((item, index)=> { %>
                                                <tr>
                                                    <td>
                                                        <%=item.id_gudang%>
                                                    </td>
                                                    <td>
                                                        <%=item.nama_gudang%>
                                                    </td>
                                                    <td>
                                                        <%=item.alamat_gudang%>
                                                    </td>
                                                    <td>
                                                        <div class="d-flex gap-3">
                                                            <a href="javascript:void(0);" class="text-pri"><i class="fa-solid fa-print"></i></a>
                                                            <a href="/gudang/edit/<%= item.id_gudang %>" class="text-success"><i class="fa-solid fa-pencil"></i></a>
                                                            <a href="/gudang/delete/<%= item.id_gudang %>" class="text-danger"><i class="fa-solid fa-trash"></i></a>
                                                        </div>
                                                    </td>
                                                </tr>
                                                <% }) %>
                                            </tbody>
                                        </table>
                                    </div>

                                    <div class="row mb-2">
                                        <div class="col-sm-4">
                                            <div class="text-sm-left">
                                                <a href="/gudang/add" class="btn btn-success btn-rounded waves-effect waves-light mb-2 me-2"><i class="fa-solid fa-plus"></i> Tambah Gudang </a>
                                            </div>
                                        </div>
                                        <div class="col-sm-8">
                                            <ul class="pagination pagination-rounded justify-content-end mb-2">
                                                
                                                <li class="page-item">
                                                    <%if (page !=1 ) { %>
                                                        <a class="page-link"
                                                            href="<%=url.replace(`page=${page}`,`page=${parseInt(page)-1}`)%>"><i class="fa-solid fa-angle-left"></i></a>
                                                        <% } %>
                                                </li>

                                                <% for (let i = 1; i <=pages; i++) { %>
                                                <li class="page-item <%= i == page ? 'active' : '' %>" aria-current="page">
                                                    <a class="page-link" href="<%=url.includes('gudang' url.replace(`page=${page}`,`gudang?page=${i}`)) %>">
                                                        <!-- `/gudang?page=${page}${filter}` -->
                                                        <%= i %>
                                                    </a>
                                                </li>
                                                <% } %>

                                                
                                                <!-- <% for (let i=1; i <=pages; i++) { %> 
                                                    <li class="page-item<%= i == page ? ' active' : '' %>"><a class="page-link" href="<%= url.includes(`&page=${page}`) ? url.replace(`&page=${page}`, `&page=${i}`) : `/gudang?page=${i}` %>"
                                                     ><%= i %>
                                                    </a>
                                                </li>
                                                     <% } %> -->

                                                <li class="page-item">
                                                    <%if (page <=pages && page !=pages) { %>
                                                        <a class="page-link"
                                                            href="<%=url.replace(`page=${page}`,`page=${parseInt(page)+1}`)%>"><i class="fa-solid fa-angle-right"></i></a>
                                                        <% } %>
                                                </li>
                                            </ul>

                                            
                                        </div><!-- end col-->
                                    </div>

                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- end row -->
                </div>
                <!-- container-fluid -->
            </div>
            <!-- End Page-content -->

            <footer class="footer">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-sm-6">
                            <script>document.write(new Date().getFullYear())</script> Â© Skote.
                        </div>
                        <div class="col-sm-6">
                            <div class="text-sm-end d-none d-sm-block">
                                Design & Develop by Themesbrand
                            </div>
                        </div>
                    </div>
                </div>
            </footer>
        </div>
        <script>

            
        </script>
        <!-- end main content-->

        <%- include('../partials/footer') %>