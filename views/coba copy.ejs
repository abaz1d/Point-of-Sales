router.get('/', async function (req, res, next) {
    try {
    let wheresg = []
    let valuesg = []
    let countg = 1

    if (req.query.cari_id) {
      wheresg.push(`id_gudang ilike '%' || $${countg++} || '%'`);
      valuesg.push(req.query.cari_id);
    }

    if (req.query.cari_nama) {
      wheresg.push(`nama_gudang ilike '%' || $${countg++} || '%'`);
      valuesg.push(req.query.cari_nama);
    }

    if (req.query.cari_alamat) {
      wheresg.push(`alamat_gudang ilike '%' || $${countg++} || '%'`);
      valuesg.push(req.query.cari_alamat);
    }

      sqlg = 'SELECT * FROM gudang'
      if (wheresg.length > 0) {
        sqlg += ` WHERE ${wheresg.join(' AND ')}`
      }

      sqlg += ` ORDER BY id_gudang`
      
      const { rows } = await db.query(sqlg,valuesg);

      res.render('gudang/list', {
        rows,

        query: req.query
      })
    } catch (e) {
      res.send(e)
    }
  });